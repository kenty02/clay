{
  "manifest_version": 3,
  "name": "Clay",
  "description": "Your toolset for exploring web",
  "version": "0.1.0",
  "icons": {
    "16": "icon-16.png",
    "32": "icon-32.png"
  },
  "permissions": [
    "tabs",
    "activeTab",
    "history",
    "webNavigation",
    "nativeMessaging",
    "tabGroups",
    "notifications",
    "<all_urls>"
  ],
  "host_permissions": [
    "<all_urls>"
  ],
  "action": {
    "default_popup": "index.html"
  },
  "background": {
    "service_worker": "src/background.ts",
    "type": "module"
  },
  "content_scripts": [
    {
      "js": [
        "src/content.ts"
      ],
      "matches": [
        "<all_urls>"
      ]
    }
  ],
  "commands": {
    "open-view": {
      "suggested_key": {
        "default": "Alt+Q",
        "mac": "Alt+Q"
      },
      "description": "Open Clay viewer"
    }
  },
  "externally_connectable": {
    "matches": [
      "about:blank"
    ]
  },
  "key": ""
}
